    </activity>
    <activity android:name="com.example.demo.view.ScrollGridActivity" />
    <activity android:name="com.example.demo.view.RecyclerViewListActivity" />
    <activity android:name="com.example.demo.view.ExpandableListViewActivity" />
    <activity android:name="com.example.demo.view.ExpandableGridViewActivity" />
  </application>

</manifest>